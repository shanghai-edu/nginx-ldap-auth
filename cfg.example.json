{
    "ldap": {
        "addr": "ldap.example.org:389",
        "baseDn": "dc=example,dc=org",
        "bindDn": "cn=manager,dc=example,dc=org",
        "bindPass": "password",
        "authFilter": "(&(uid=%s))",
        "attributes": ["uid", "cn", "mail"],
        "tls":        false,
        "startTLS":   false
    },
    "control":{
        "ipAcl":{
            "deny":[],
            "direct":[]
        },
        "timeAcl":{
            "deny":[],
            "direct":[]
        },
        "allowUser":["user1"]
    },
    "http": {
        "debug":false,
        "ips":["127.0.0.1"],
        "listen": "0.0.0.0:8080"
    }
}